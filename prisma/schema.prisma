// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Pokemon {
  id               Int     @id
  name             String  @unique
  front_sprite_url String?
  back_sprite_url  String?
}
